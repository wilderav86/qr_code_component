  },
  "homepage": "https://github.com/wilderav86/qr_code_component#readme",
  "dependencies": {
    "sass": "^1.56.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "browser-sync": "^2.27.10",
    "cssnano": "^5.1.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19"
  }
}

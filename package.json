{
  "name": "qr_code_component",
  "version": "1.0.0",
  "description": "This is a solution to the [QR code component challenge on Frontend Mentor](https://www.frontendmentor.io/challenges/qr-code-component-iux_sIO_H). Frontend Mentor challenges help you improve your coding skills by building realistic projects.",
  "main": "index.js",
  "scripts": {
    "build": "sass style.scss style.css",
    "postbuild": "postcss style.css -u autoprefixer cssnano -r --no-map",
    "dev:build": "sass -w style.scss style.css",
    "dev:serve": "browser-sync start -sw",
    "dev": "run-p dev:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wilderav86/qr_code_component.git"
  },
